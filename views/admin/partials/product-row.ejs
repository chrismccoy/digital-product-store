<tr data-id="<%= product.id %>" class="bg-white">
  <td class="p-4 font-bold"><%= product.name %></td>
  <td class="p-4">$<%= product.price %></td>
  <td class="p-4"><%= product.text %></td>
  <td class="p-4 space-x-4">
    <button
      type="button"
      class="edit-btn font-bold uppercase underline hover:bg-yellow-300"
    >
      Edit
    </button>
    <button
      type="button"
      class="delete-btn font-bold uppercase underline hover:bg-yellow-300"
    >
      Delete
    </button>
  </td>
</tr>
